    "mailgun-js": "^0.22.0",
    "md5": "^2.2.1",
    "moment": "^2.24.0",
    "mongoose": "^5.7.6",
    "mongoose-paginate-v2": "^1.3.6",
    "multer": "^1.4.2",
    "nodemailer": "^6.3.1",
    "nodemon": "^1.19.4",
    "nuxt": "^2.10.2",
    "passport": "^0.4.0",
    "passport-http": "^0.3.0",
