{
  "name": "expressnuxtvanilla",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev": "concurrently 'npm run dev:app' 'npm run dev:server'",
    "dev:app": "cross-env BASE_URL=http://localhost:8080/api nuxt dev --port 3000",
    "dev:server": "cross-env DEBUG=express:* && nodemon --ignore app -r @babel/register -r @babel/polyfill -r dotenv/config src/bin/server.js",
    "repl": "cross-env DEBUG=express:* && node -r @babel/register -r @babel/polyfill -r dotenv/config src/bin/repl.js",
    "build": "babel src -d server && nuxt build",
    "prod": "npm run build && npm start",
    "lint": "eslint . --ext .vue --ext .js --quiet",
    "test": "node -r @babel/register -r @babel/polyfill -r dotenv/config src/bin/test.js",
    "start": "nuxt start",
    "run:docker": "docker run --env-file .env.list -it -p 5000:8080 adifia:latest",
    "build:docker": "docker build -t adifia .",
    "docker": "npm run build:docker && npm run run:docker"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint"
    }
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@babel/cli": "^7.6.4",
    "@babel/core": "^7.6.4",
    "@babel/plugin-transform-runtime": "^7.7.6",
    "@babel/polyfill": "^7.6.0",
    "@babel/preset-env": "^7.6.3",
    "@babel/register": "^7.6.2",
    "@babel/runtime": "^7.7.7",
    "@editorjs/editorjs": "^2.15.1",
    "@editorjs/image": "^2.3.3",
    "@hapi/boom": "^8.0.1",
    "@nuxtjs/axios": "^5.7.1",
    "@nuxtjs/dotenv": "^1.4.1",
    "@nuxtjs/vuetify": "^1.9.0",
    "@sendgrid/mail": "^6.4.0",
    "@status/codes": "^1.2.1",
    "@types/express": "^4.17.1",
    "@vuex-orm/core": "^0.32.5",
    "alertify": "^0.3.0",
    "bcrypt": "^5.0.0",
    "body-parser": "^1.19.0",
    "cloudinary": "^1.17.0",
    "concurrently": "^5.0.0",
    "cors": "^2.8.5",
    "cross-env": "^6.0.3",
    "dotenv": "^8.2.0",
    "eslint": "^6.5.1",
